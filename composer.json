{
    "name": "buroq/helper",
    "description": "PHP Helpers for easy to use",
    "keywords": [
        "php",
        "helper",
        "function",
        "array",
        "device",
        "string",
        "utilities",
        "yaml"
    ],
    "type": "library",
    "require": {
      "php": ">=5.6",
      "mobiledetect/mobiledetectlib": "^2.8",
      "jaybizzle/crawler-detect": "^1.2",
      "mustangostang/spyc": "^0.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ransrue",
            "email": "admin@buroq.app"
        }
    ],
    "autoload": {
        "psr-4": {
            "BUROQ\\Helper\\": "src/Helper"
        },
        "files": [
          "src/helper.php"
        ]
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5",
        "cvuorinen/phpdoc-markdown-public": "^0.2",
        "mikey179/vfsstream": "^1.5.0"
    },
    "config": {
        "platform": {
            "php": "5.6.0"
        },
        "optimize-autoloader": true
    }
}
